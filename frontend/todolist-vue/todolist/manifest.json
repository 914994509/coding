{
  "name": "todolist",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "todo list",
  "icons": {
    "16": "icons/blue/todolist_16_16.png",
    "32": "icons/blue/todolist_32_32.png",
    "48": "icons/blue/todolist_48_48.png",
    "128": "icons/blue/todolist_128_128.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ]
}